<UserControl x:Class="Client.View.TestUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Client.View"
             mc:Ignorable="d" 
             d:DesignHeight="160" d:DesignWidth="200">
    <Grid>
        <StackPanel>
            <Button  Content="{Binding Test1Text}"  Command="{Binding Test1Cmd}"/>
            <Button  Content="{Binding Test2Text}"  Command="{Binding Test2Cmd}"/>
            <Button  Content="{Binding Test3Text}"  Command="{Binding Test3Cmd}"/>
            <TextBlock Text="{Binding Index}"/>
        </StackPanel>
    </Grid>
</UserControl>
